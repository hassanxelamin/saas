    "lint:fix": "eslint './src/**/*.{ts,tsx}' --fix",
    "prettier-check": "prettier --check './src/**/*.{ts,tsx}'",
    "prettier-format": "prettier --config .prettierrc './src/**/*.{ts,tsx}' --write",
    "test": "jest",
    "coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "cz": "git-cz"
  },
